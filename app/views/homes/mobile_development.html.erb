    <div data-controller="case_study_rotator" data-target="page.loadOnScroll page.section" data-theme="monoBaseBlack" data-bgcolor="#ffffff" class="scroll-transition" style="background-color: rgb(255, 255, 255); color: rgb(0, 0, 0);">
